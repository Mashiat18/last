<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>../view/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{studentController.isNextPage}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>../view/success.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>

    <navigation-rule>
        <from-view-id>../view/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{studentController.isNextPage2}</from-action>
            <from-outcome>success2</from-outcome>
            <to-view-id>../view/success2.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>../view/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{studentController.isNextPage3}</from-action>
            <from-outcome>success3</from-outcome>
            <to-view-id>../view/success3.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

</faces-config>